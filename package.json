{
  "name": "api-rest_ts",
  "version": "1.0.0",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "db:create": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "db:reset": "sequelize db:drop && npm run db:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.30",
    "@types/sequelize": "^4.28.20",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3"
  }
}
